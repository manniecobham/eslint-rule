{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "my-plugin",
    "jsdoc"
  ],
  "extends": [
    "plugin:jsdoc/recommended",
    "plugin:vue/essential",
    "eslint:recommended",
    "@vue/typescript/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "my-plugin/double-quotes": "error",
    "my-plugin/no-delete-eslint-prettier": "error",
    "my-plugin/jsdoc-for-functions": "error"
  }
}